{
    "Name":            "PieQuarter Truchet",
    "Description":     "PieQuarter Truchet pattern: Color names retrieved with the help of 'Name that color' (https://chir.ag/projects/name-that-color)",
    "Categories":      ["Truchet"],
    "GridSize":        { "width": 27, "height": 15 },
    "CellSize":        { "width": 20, "height": 20 },
    "MaxCellValue":    7,
    "BackgroundColor": "#000000",
	
	"@include": { "src": "includes/color_palette_Sepia.json", "#type": "COLOR_PALETTE" },
	
    "Shapes": {	
	    "0": [ { "Shape": "Ellipse", "FillColor": "$DoubleSpanishWhite", "Arc": { "start": 0, "end": 90 } } ],
        "1": [ { "Shape": "Ellipse", "FillColor": "$IndianKhaki", "Arc": { "start": 90, "end": 180 } } ],		
		"2": [ { "Shape": "Ellipse", "FillColor": "$Shadow", "Arc": { "start": 180, "end": 270 } } ],				
        "3": [ { "Shape": "Ellipse", "FillColor": "$Gimblet", "Arc": { "start": 270, "end": 0 } }  ],			   
               
        "4": [ { "Shape": "Ellipse", "FillColor": "$ShingleFawn", "Arc": { "start": 0, "end": 90 } } ], 
        "5": [ { "Shape": "Ellipse", "FillColor": "$Alto", "Arc": { "start": 90, "end": 180 } } ],
		"6": [ { "Shape": "Ellipse", "FillColor": "$Gray", "Arc": { "start": 180, "end": 270 } } ], 
        "7": [ { "Shape": "Ellipse", "FillColor": "$Scorpion", "Arc": { "start": 270, "end": 0 } } ] 
    }               
}